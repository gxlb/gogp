;this is exactlly an ini file
;this is an example of gogp reverse usage: generate .gp file form .gpg and .go file, sectionname must be GOGP_REVERSE
;GOGP_REVERSE section is used to do the next thing, gen .go file from .gpg and .gp file
[GOGP_REVERSE_stack]
GOGP_Name=stack
PACKAGE=package examples
STACK_ELEM=GOGPStackElem
STACK_NAME_PREFIX=GOGPStackNamePrefix
SHOW_COMMENT=//GOGPShowComment

;this will be ignore by gogp tool
[GOGP_IGNORE]
PACKAGE=package examples
STACK_ELEM=GOGPStackElem
STACK_NAME_PREFIX=GOGPStackNamePrefix
SHOW_COMMENT=//GOGPShowComment

[int]
GOGP_Name=int
PACKAGE=package examples
STACK_ELEM=int
STACK_NAME_PREFIX=Int
SHOW_COMMENT=//

[string]
GOGP_Name=string
PACKAGE=package examples
STACK_ELEM=string
STACK_NAME_PREFIX=String
SHOW_COMMENT=//